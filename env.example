# OpenAI API Key (provided in requirements)
OPENAI_API_KEY=your_openai_api_key_here

# Cal.com API Key (get from https://app.cal.com/settings/developer/api-keys)
CALCOM_API_KEY=your_calcom_api_key_here

# Cal.com User Email (your cal.com account email)
CALCOM_USER_EMAIL=your_email@example.com

# Cal.com Event Type ID (get from your cal.com event types)
CALCOM_EVENT_TYPE_ID=your_event_type_id

# Cal.com API Base URL
CALCOM_API_BASE_URL=https://api.cal.com/v2

# LangSmith Tracing (Optional - for debugging and monitoring)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=cal.com-chatbot

